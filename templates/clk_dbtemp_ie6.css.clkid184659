/*-----{global link colors}-----*/

a
{
color: #A1030C;	
}

.fakeLink{
text-decoration:underline;
cursor:pointer;
}

/*-----{Page: body centered}-----*/
body
{
	background-color: #393939;
	margin-top: 0px;
	margin-right: auto;
	margin-left: auto;
	width: 994px;
	font-family: Arial, sans-serif;
	font-style: normal;
	behavior: url("/templates/csshover.htc");
}

/*-----{Header}-----*/
#headWrapper
{
	width: 943px;
	min-height: 81px;
	padding-right: 25px;
	padding-left: 26px;
	position: absolute;
	top: 0px;
	left: auto;
	right: auto;
	background-color: #f2f2f2;
        z-index: 1;
}

/* left side with logo */
#headLeft
{
	width: 343px;
	height: 81px;
	float: left;
	padding-top: 10px;
}

#headRight /* right side with navigation */
{
	width: 600px;
	height: 81px;
	float: right;
	text-align: right;
}

#tagLine /* tag line */
{
	width: 343px;
	text-align: left;
	font-size: 12px;
	color: #d4181f;
	margin-top: 10px;
	overflow: hidden;
}

#headRightTop /* right top */
{
	width: 600px;
	height: 21px;
	text-align: right;
	font-size: 10px;
	color: #666;
	padding-top: 6px;
}

#headRightTopWrapper
{
	width: 600px;
	height: 21px;
	overflow: hidden;
}

#headRightTopLinks /* The top links box */
{
	float: right;
	text-align: right;
}

a.headRightTopNav
{
	font-size: 10px;
	color: #666;
	text-decoration: none;
}

a.headRightTopNav:hover
{
	text-decoration: underline;
}

span.smallLinksWrapper
{
	position: relative;
	top: 4px;
}

#headRightTopForm /* The top form box */
{
	float: right;
	margin-left: 10px;
}

#headRightTop form.clk_search /* form */
{
	width: 136px;
	height: 18px;
	margin: 0px;
	padding: 0px;
}

#headRightTop input.searchClk /* input */
{
	width: 112px;
	height: 14px;
	padding: 2px 0px 2px 4px;
	border: 0px;
	margin-top: 0px;
	background-image: url($cms.media("searchClkBgd.gif", "designimages").url);
	background-repeat: no-repeat;
	background-color: transparent;
	font-size: 10px;
	color: #666;
}

#headRightTop input.submitClk /* submit */
{
	width: 20px;
	height: 18px;
	border: 0px;
	margin: 0px;
}

#headRightBot /* right bottom */
{
	width: 600px;
	height: 54px;
	text-align: right;
}

/*----------{Navigation}----------*/
#nav_box
{
	width: 598px;
	height: 25px;
	text-align: left;
	margin-top: 25px;
	margin-right: 2px;
}

.dropMenuBoxWrapper
{
	float: right;
}

 /* top vertical dividers */
.navDivider
{
	float: right;
}

 /* drop horizontal dividers (Ie6 really hates this) */
li.liDividers
{
	margin: 0px;
	padding: 0px;
	overflow: hidden;
	display: inline;
	width: 181px;
}

div.navItemsDivider
{
	width: 171px;
	height: 1px;
	margin: 3px 0px 3px 5px;
	padding: 0px;
	background-color: #CCC;
	overflow: hidden;
}


.navItemsEnd
{
	width: 161px;
	height: 1px;
	background-color: transparent;
	overflow: hidden;
	margin-left: 10px;
	padding: 0px;
}


.navItems
{
	width: 161px;
	background-color: transparent;
	padding-left: 5px;
	margin: 0px;
	overflow: hidden;
}

.navItemsLeft
{
	width: 136px;
	padding: 5px 20px 7px 5px;
	overflow: hidden;
}

span.leftNavHeadings
{
	margin-left: 15px;
	margin-top: 5px;
	color: #666;
}

.clear
{
	clear: both;
}

/*----------Level 0-----------*/
#nav_box ul.ullevel0
{
	list-style: none;
	padding: 0px;
	margin: 0px;
	position: relative;
	top: 3px;
}

#nav_box ul.ullevel0 li.lilevel0
{
	width: 85px;
	height: 23px;
	padding-left: 15px;
	margin-right: 1px;
	margin-left: 1px;
	color: #000;
	text-align: left;
	overflow: hidden;
	cursor: pointer;
}

#nav_box ul.ullevel0:hover li.lilevel0
{
	color: #FFF;
	background-image: url($cms.media("main_nav_over.gif", "designimages").url);
	background-repeat: repeat-x;
	background-position: 0px 0px;
}

#nav_box ul.ullevel0 li.lilevel0Select
{
	width: 85px;
	height: 23px;
	padding-left: 15px;
