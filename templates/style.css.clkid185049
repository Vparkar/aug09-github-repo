/*----------Level 0-----------*/
#nav_box ul.ullevel0
{
	list-style: none;
	padding: 0px;
	margin-top: 3px;
}

#nav_box ul.ullevel0 li
{
font-size:1em;
}

#nav_box ul.ullevel0 li.lilevel0, #nav_box ul.ullevel0 li.lilevel0 a
{
	height: 23px;
	padding-left: 6px;
	padding-right:6px; 
	margin-right: 1px;
	margin-left: 1px;
	color: #000;
	text-align: left;
	cursor: pointer;
}

#nav_box ul.ullevel0:hover li.lilevel0, #nav_box ul.ullevel0:hover li.lilevel0 a
{
	color: #FFF;
	background-image: url(http://media.animalbites.com/designimages/main_nav_over.gif);
	background-repeat: repeat-x;
	background-position: 0px 0px;
}

#nav_box ul.ullevel0 li.lilevel0Select, #nav_box ul.ullevel0 li.lilevel0Select a
{
	height: 23px;
	padding-left: 15px;
	padding-right:15px; 
margin-right: 1px;
	margin-left: 1px;
	color: #fff;
	text-align: left;
	background-image: url(http://media.animalbites.com/designimages/main_nav_selected.gif);
	background-repeat: repeat-x;
	background-position: 0px 0px;
	cursor: pointer;
}

#nav_box ul.ullevel0 li.lilevel0Select:hover, #nav_box ul.ullevel0 li.lilevel0Select:hover a
{
	width: 85px;
	height: 23px;
	padding-left: 15px;
	margin-right: 1px;
	margin-left: 1px;
	color: #fff;
	text-align: left;
	background-image: url(http://media.animalbites.com/designimages/main_nav_over.gif);
	background-repeat: repeat-x;
	background-position: 0px 0px;
}

/*----------Level 1-----------*/
#nav_box ul.ullevel0 li.lilevel0 ul.ullevel1, #nav_box ul.ullevel0 li.lilevel0Select ul.ullevel1
{
	list-style: none;
	padding: 0px;
	margin: 0px;
}

#nav_box ul.ullevel0 li.lilevel0 ul.ullevel1 li.lilevel1, #nav_box ul.ullevel0 li.lilevel0Select ul.ullevel1 li.lilevel1
{
	width: 181px;
	display: none;
}

#nav_box ul.ullevel0 li.lilevel0:hover ul.ullevel1 li.lilevel1, #nav_box ul.ullevel0 li.lilevel0Select:hover ul.ullevel1 li.lilevel1
{
	display: block;
	position: relative;
	z-index: 100;
	top: 3px;
	left: -15px;
	padding: 0px;
}


/*----------Level 2-----------*/
#nav_box ul.ullevel0 li.lilevel0 ul.ullevel1 li.lilevel1 ul.ullevel2, #nav_box ul.ullevel0 li.lilevel0Select ul.ullevel1 li.lilevel1 ul.ullevel2
{
	list-style: none;
	padding: 0px;
	background-color: transparent;
	background-image: url(http://media.animalbites.com/designimages/nav_ul_bgd.png);
	background-repeat: repeat;
	border: 1px #dedede solid;
}

#nav_box ul.ullevel0 li.lilevel0 ul.ullevel1 li.lilevel1 ul.ullevel2 li.lilevel2, #nav_box ul.ullevel0 li.lilevel0Select ul.ullevel1 li.lilevel1 ul.ullevel2 li.lilevel2
{
	text-align: left;
	padding: 0px 0px 0px 5px;
}

#nav_box ul.ullevel0 li.lilevel0 ul.ullevel1 li.lilevel1 ul.ullevel2 li.lilevel2:hover, #nav_box ul.ullevel0 li.lilevel0Select ul.ullevel1 li.lilevel1 ul.ullevel2 li.lilevel2:hover
{
	background-image: url(http://media.animalbites.com/designimages/drop_nav_bgd.png);
	background-repeat: no-repeat;
	background-position: 10px 100%;
}

#nav_box ul.ullevel0 li.lilevel0 ul.ullevel1 li.lilevel1 ul.ullevel2 li.lilevel2 a, #nav_box ul.ullevel0 li.lilevel0Select ul.ullevel1 li.lilevel1 ul.ullevel2 li.lilevel2 a
{
	display: block;
	font-size: 12px;
	color: #A1030C;
	text-decoration: none;
}

#nav_box ul.ullevel0 li.lilevel0 ul.ullevel1 li.lilevel1 ul.ullevel2 li.lilevel2:hover a, #nav_box ul.ullevel0 li.lilevel0Select ul.ullevel1 li.lilevel1 ul.ullevel2 li.lilevel2:hover a
{
	color: #FFF;
}

#nav_box ul.ullevel0 li.lilevel0 ul.ullevel1 li.lilevel1 ul.ullevel2 li.lilevel2 div.navItems div.navItemsLeftArrow, #nav_box ul.ullevel0 li.lilevel0Select ul.ullevel1 li.lilevel1 ul.ullevel2 li.lilevel2 div.navItems div.navItemsLeftArrow
{
	width: 131px;
	padding: 7px 20px 7px 10px;
	background-image: url(http://media.animalbites.com/designimages/gray_arrow.png);
	background-repeat: no-repeat;
	background-position: 150px 50%;
}

#nav_box ul.ullevel0 li.lilevel0 ul.ullevel1 li.lilevel1 ul.ullevel2 li.lilevel2:hover div.navItems div.navItemsLeftArrow, #nav_box ul.ullevel0 li.lilevel0Select ul.ullevel1 li.lilevel1 ul.ullevel2 li.lilevel2:hover div.navItems div.navItemsLeftArrow
{
	background-image: url(http://media.animalbites.com/designimages/white_arrow.png);
}

/*----------Level 3-----------*/
#nav_box ul.ullevel0 li.lilevel0 ul.ullevel1 li.lilevel1 ul.ullevel2 li.lilevel2 ul.ullevel3, #nav_box ul.ullevel0 li.lilevel0Select ul.ullevel1 li.lilevel1 ul.ullevel2 li.lilevel2 ul.ullevel3
{
	position: absolute;
	left: -999em;
	z-index: 2000;
	width: 181px;
	list-style: none;
	padding: 0px;
	background-color: transparent;
	background-image: url(http://media.animalbites.com/designimages/nav_ul_bgd.png);
	background-repeat: repeat;
	border: 1px #dedede solid;
}

#nav_box ul.ullevel0 li.lilevel0 ul.ullevel1 li.lilevel1 ul.ullevel2 li.lilevel2:hover ul.ullevel3, #nav_box ul.ullevel0 li.lilevel0Select ul.ullevel1 li.lilevel1 ul.ullevel2 li.lilevel2:hover ul.ullevel3
{
	display: block;
	position: absolute;
	top: auto;
	left: 178px;
	z-index: 2000;
}

#nav_box ul.ullevel0 li.lilevel0 ul.ullevel1 li.lilevel1 ul.ullevel2 li.lilevel2 ul.ullevel3 li.lilevel3, #nav_box ul.ullevel0 li.lilevel0Select ul.ullevel1 li.lilevel1 ul.ullevel2 li.lilevel2 ul.ullevel3 li.lilevel3
{
	text-align: left;
	padding: 0px 0px 0px 5px;
}

#nav_box ul.ullevel0 li.lilevel0 ul.ullevel1 li.lilevel1 ul.ullevel2 li.lilevel2 ul.ullevel3 li.lilevel3:hover, #nav_box ul.ullevel0 li.lilevel0Select ul.ullevel1 li.lilevel1 ul.ullevel2 li.lilevel2 ul.ullevel3 li.lilevel3:hover
{
	background-image: url(http://media.animalbites.com/designimages/drop_nav_bgd.png);
	background-repeat: no-repeat;
	background-position: 10px 100%;
}

#nav_box ul.ullevel0 li.lilevel0 ul.ullevel1 li.lilevel1 ul.ullevel2 li.lilevel2 ul.ullevel3 li.lilevel3 a, #nav_box ul.ullevel0 li.lilevel0Select ul.ullevel1 li.lilevel1 ul.ullevel2 li.lilevel2 ul.ullevel3 li.lilevel3 a
{
	display: block;
	font-size: 12px;
	color: #A1030C;
	text-decoration: none;
}

#nav_box ul.ullevel0 li.lilevel0 ul.ullevel1 li.lilevel1 ul.ullevel2 li.lilevel2 ul.ullevel3 li.lilevel3:hover a, #nav_box ul.ullevel0 li.lilevel0Select ul.ullevel1 li.lilevel1 ul.ullevel2 li.lilevel2 ul.ullevel3 li.lilevel3:hover a
